{
  "name": "oku-api-project",
  "version": "1.0.0",
  "description": "A DeFi assistant using the Oku API for Uniswap v3 pools.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
